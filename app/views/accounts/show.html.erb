<h1>Listing accounts</h1>

<table>
  <tr>
    <th>Account type</th>
    <th>Money</th>
    <th>Money type</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

  <% @accounts.each do |account| %>
      <tr>
        <td><%= account.account_type %></td>
        <td><%= account.money %></td>
        <td><%= account.money_type %></td>
      </tr>
  <% end %>
</table>

